	-f /run/mysqld \
	-f /root \
	-f /usr/bin/mariadb-install-db \
	-f /usr/bin/my_print_defaults \
	-f /usr/share/mysql/ \
	-f /etc/hosts \
	-f /etc/mysql \
	-f /usr/bin/resolveip \
	-f /etc/group \
	-f /usr/bin/gpg \
